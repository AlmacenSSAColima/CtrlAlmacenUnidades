<div id="modal_insumo_det"></div>


@section scripts {
    <script>

        $(window).on('load', function () {
            setTimeout(function () {
                //close_loading();
                $("#loader_movil_").css({ visibility: "hidden", opacity: "0" })
            }, 1200);
        });

        $("#buscar_item").keyup(function (e) {
            _this = this;
            $("#ctn_items .itms").each(function (i, e) {
                if ($(this).text().toLowerCase().indexOf(_this.value.toLowerCase()) === -1)
                    $(this).hide();
                else
                    $(this).show();
            });
        });

        function showProgramacion(id_el) {
            show_loading();
            setTimeout(function () {
                close_loading();
                $("#" + id_el).show();
            }, 800);
        }
        function hideProgramacion(id_el) {
            $("#" + id_el).hide();
        }

        function verSolicitudVSsurtido(cve, id_unidad)
        {
            //URL que mostrara la modal
            var url = '/Existencia/GetSolicitadoVsSurtido?cve='+cve+'&unidad=' + id_unidad;
            //Mostrar porcesando
            show_loading();
            $.ajax({
                type: 'GET',
                url: url,
                success: function (data) {
                    $("#modal_insumo_det").html(data);
                    $('#ModalInsumoSurtido').modal('show');
                    close_loading();
                },
                error: function (ex) {
                    console.log(ex);
                }
            });
        }
    </script>
}

