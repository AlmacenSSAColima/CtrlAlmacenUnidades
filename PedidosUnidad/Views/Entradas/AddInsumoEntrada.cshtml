@model PedidosUnidad.Models.AddInsumoForm
@{
    Layout = null;
}

<!DOCTYPE html>

<style>
    .linsumo {
        cursor: pointer;
    }

        .linsumo:hover {
            background: #eee;
        }

    .prw_insumo {
       float:left; cursor:pointer;
    }
    .prw_insumo:hover {
        color:#3F51B5;
